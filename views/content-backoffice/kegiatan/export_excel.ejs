
<div id="all" style="display:none">
  <table border="1" cellpadding="5" cellspacing="5" style="width: 100%;border-collapse: collapse;">
      <thead>
        <tr>
          <th>No.</th>
          <th>Kegiatan</th>
          <th>Jenis Pekerjaan</th>
          <th>Lokasi</th>
          <th>Kecamatan</th>
          <th>Kelurahan</th>
          <th>Volume</th>
          <th>Jenis Anggaran</th>
          <th>Jumlah Anggaran (Rp)</th>
          <th>Pelaksana</th>
          <th>Kesesuain Dengan PERWALI</th>
          <th>Keterangan</th>
          <th>Aprroveal</th>
        </tr>
      </thead>

      <tbody>
        <% for(let i=0; i < respon.length; i++) {%>
          <tr>
            <td><%= i+1%></td>
            <td><%= respon[i].kegiatanPrioritas%></td>
            <td><%= (respon[i].jeni)?respon[i].jeni.jenis:''%></td>
            <td><%= respon[i].lokasi%></td>
            <td><%= respon[i].kec%></td>
            <td><%= respon[i].kel%></td>
            <td><%= respon[i].volume%></td>
            <td><%= respon[i].jenisAnggaran%></td>
            <td><%= respon[i].jumlahAnggaran%></td>
            <td><%= respon[i].pelaksana%></td>
            <td><%= (respon[i].kesesuaian)?'Ya':'Tidak'%></td>
            <td><%= respon[i].keterangan%></td>
            <td><%= (respon[i].approval)?'Ya':'Tidak'%></td>
            
    
          </tr>
          <% } %>
      </tbody>
  </table>
</div>
<!-- END PAGE CONTENT -->



<script type="text/javascript">



    //getting data from our table
    var data_type = 'data:application/vnd.ms-excel';
    var table_div = document.getElementById('all');
    var table_html = table_div.outerHTML.replace(/ /g, '%20');

    var a = document.createElement('a');
    a.href = data_type + ', ' + table_html;
    a.download = 'Kegiatan Prioritas' + Math.floor((Math.random() * 9999999) + 1000000) + '.xls';
    a.click();
    setTimeout("window.close();", 5000);
    // window.close();
    // location.href('/manajemen_paket_pekerjaan')
</script>